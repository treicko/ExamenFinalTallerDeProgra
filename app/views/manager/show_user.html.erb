<div class="container">
  <div class="bs-docs-section">
    <h1 id="tables" class="page-header">Ver Usuario</h1>
    <div class="bs-example">
		<p id="notice"><%= notice %></p>

		<p>
		  <strong>Id:</strong>
		  <%= @user.id %>
		</p>
		<p>
		  <strong>Correo:</strong>
		  <%= @user.email %>
		</p>

		<p>
		  <strong>Rol:</strong>
		  <%= if @user.has_role? :admin then "Administrador" else "Usuario" end %>
		</p>
		<p>
		  <strong>Estado:</strong>
		  <%= if @user.user_enable then "Habilitado" else "Des-Habilitado" end %>
		</p>
		<p>
		  <strong>Cambio Contrace√±a en:</strong>
		  <%= @user.reset_password_sent_at %>
		</p>
		<p>
		  <strong>Ultimo ingreso en:</strong>
		  <%= @user.last_sign_in_at %>
		</p>
		<p>
		  <strong>Creado en:</strong>
		  <%= @user.created_at %>
		</p>
		<p>
		  <strong>Modificado en:</strong>
		  <%= @user.updated_at %>
		</p>
 
		<a href="/manager/users_list"><i class="icon-reorder"></i> Volver a la Lista De Usuarios</a>
    </div>
  </div>
</div>

